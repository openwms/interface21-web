web:    java -Xmx400m -Xss256k $JAVA_OPTS -Dserver.port=$PORT -Dspring.profiles.active=HEROKU-DEV -jar anthrododa/sample/oauth2/resourceserver/target/resserver.jar
autz:    java -Xmx400m -Xss256k $JAVA_OPTS -Dserver.port=$PORT -Dspring.profiles.active=HEROKU-DEV -jar anthrododa/sample/oauth2/authorizationserver/target/authserver.jar
